#!/bin/sh
# should be symlinked from /usr/local/bin/xdg-open

echo `date +%F` "$0" "$@" >> /tmp/xdg-open

case "$@" in
*docx|*doc|*xlsx|*xls|*pptx|*ppt)
	exec /home/himdel/bin/gdocs.sh "$@"
	;;
*jpg|*gif|*jpeg|*png|*JPG)
	exec pqiv "$@"
	;;
http:*|https:*)
	exec $BROWSER "$@"
	;;
magnet:*)
	exec magnet "$@"
	;;
*webm)
	exec mpv "$@"
	;;
*)
	exec /usr/bin/xdg-open "$@"
	;;
esac
